        <Button x:Name="selectFilesButton" Content="Select Files" HorizontalAlignment="Left" Margin="376,115,0,0" VerticalAlignment="Top" Width="88" Height="32" Click="selectFilesButton_Click"/>
        <TextBox x:Name="passwordTextBox" HorizontalAlignment="Left" Height="25" Margin="376,152,0,17" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="88"/>
        <Button x:Name="passwordGenButton" Content="Password&#xa;Generator" HorizontalAlignment="Left" Margin="469,41,0,0" VerticalAlignment="Top" Width="88" Height="136" Click="passwordGenButton_click"/>
    </Grid>
</Window>
